  <div class="content" style="width:100%; margin-left:0px">
        
      <div class="header">
            
            <h1 class="page-title">每日汇报查看</h1>
        </div>
        

        <div class="container-fluid">
            <div class="row-fluid">
                    
<div class="btn-toolbar">
    
    <form action="" method="post">
		时间：<input type="text" name="date" />
		<input type="submit" name="chaxun" id="chaxun" value="查询" />
	</form>
    
  <div class="btn-group">
  </div>
</div>
<div class="well">
    <table class="table">
      <thead>
        <tr>
          <th>日期</th>
          <th>账户</th>
          <th>事件</th>
          <th>金额</th>
          <th>审批状态</th>
          <th>操作</th>
          <th style="width: 26px;"></th>
        </tr>
      </thead>
      <tbody>
      
      <c:forEach items="${sessionScope.bossFws }" var="empw">
        <tr>
          <td>${empw.financial_Workreport_Date }</td>
          <td>${empw.accountant.employee_Name }</td>
          <td>${empw.event_Id.financial_Workreport_Event_Name }</td>
          <td>${empw.financial_Workreport_Amount }</td>
          <td>${empw.state.workreport_State_Name }</td>
          <td>
        	<a href="bossGetOneFinancialWorkportAction?financial_Workreport.financial_Workreport_Id=${empw.financial_Workreport_Id }">查看</a>  
          </td>
          <td>
          <!-- 
              <a href="user.html"><i class="icon-pencil"></i></a>
              <a href="#myModal" role="button" data-toggle="modal"><i class="icon-remove"></i></a>
           -->
          </td>
        </tr>
        </c:forEach>
      </tbody>
    </table>
</div>
<div class="pagination">
    <ul>
        <li><a href="#">Prev</a></li>
        <li><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">Next</a></li>
    </ul>
</div>

<div class="modal small hide fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3 id="myModalLabel">Delete Confirmation</h3>
    </div>
    <div class="modal-body">
        <p class="error-text"><i class="icon-warning-sign modal-icon"></i>Are you sure you want to delete the user?</p>
    </div>
    <div class="modal-footer">
        <button class="btn" data-dismiss="modal" aria-hidden="true">Cancel</button>
        <button class="btn btn-danger" data-dismiss="modal">Delete</button>
    </div>
</div>


                    
