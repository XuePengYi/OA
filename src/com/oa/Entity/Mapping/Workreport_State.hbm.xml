<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.oa.Entity.Workreport_State" table="workreport_state">
		<id name="workreport_State_Id" type="java.lang.Integer" column="workreport_state_id">
			<generator class="increment"></generator>
		</id>
		<property name="workreport_State_Name" type="java.lang.String" column="workreport_state_name"></property>
	</class>
</hibernate-mapping>